{
	"name" : "alsar/symfony-extended-edition",
	"license" : "MIT",
	"type" : "project",
	"description" : "The \"Symfony Extended Edition\" distribution",
	"autoload" : {
		"psr-4" : {
			"" : [
				"src/",
				"lib/"
			]
		}
	},
	"require" : {
		"php" : ">=5.5.0",
		"symfony/symfony" : "~2.6",
		"doctrine/orm" : "~2.5@dev",
		"doctrine/dbal" : "~2.5",
		"doctrine/common" : "~2.5@dev",
		"doctrine/doctrine-bundle" : "~1.3",
		"twig/extensions" : "~1.0",
		"symfony/assetic-bundle" : "~2.3",
		"symfony/swiftmailer-bundle" : "~2.3",
		"symfony/monolog-bundle" : "~2.6",
		"sensio/distribution-bundle" : "~3.0",
		"sensio/framework-extra-bundle" : "~3.0",
		"jms/di-extra-bundle" : "~1.5",
		"doctrine/doctrine-fixtures-bundle" : "~2.2",
		"doctrine/data-fixtures" : "~1.0@dev",
		"doctrine/doctrine-migrations-bundle" : "2.1.*@dev",
		"doctrine/migrations" : "1.0.*@dev",
		"friendsofsymfony/user-bundle" : "2.0.*@alpha",
		"jms/security-extra-bundle" : "~1.5",
		"incenteev/composer-parameter-handler" : "~2.1",
		"beberlei/assert" : "~2.1",
		"alsar/specification" : "~0.1",
		"jms/serializer-bundle" : "~0.13",
		"nelmio/api-doc-bundle" : "~2.7",
		"friendsofsymfony/rest-bundle" : "~1.5"
	},
	"require-dev" : {
		"sensio/generator-bundle" : "~2.5",
		"fabpot/php-cs-fixer" : "~1.5",
		"phpunit/phpunit" : "~4.5",
		"phploc/phploc" : "~2.0",
		"sebastian/phpcpd" : "~2.0",
		"liip/rmt" : "~1.1"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
		]
	},
	"config" : {
		"bin-dir" : "bin"
	},
	"minimum-stability" : "stable",
	"extra" : {
		"symfony-app-dir" : "app",
		"symfony-web-dir" : "web",
		"symfony-assets-install" : "relative",
		"incenteev-parameters" : {
			"file" : "app/config/parameters.yml"
		}
	}
}