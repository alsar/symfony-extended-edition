<!doctype html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="{{ asset('build/app/css/app.css') }}">
    <link rel="stylesheet" href="{{ asset('build/app/css/style.css') }}">
    <link rel="icon" type="image/png" href="{{ asset('favicon.png') }}">
    {% if app.environment != 'prod' %}
        <script src="http://localhost:35729/livereload.js"></script>
    {% endif %}
</head>
<body>
<div class="container">
    <div class="row">
        {% block content %}{% endblock %}
    </div>
</div>

<script>window.jQuery || document.write('<script src="{{ asset('build/app/js/jquery.min.js') }}"><\/script>')</script>
<script src="{{ asset('build/app/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('build/app/js/main.js') }}"></script>

</body>
</html>
